<h3>Index</h3>

<p>{{link-to 'New user' 'users.new'}}</p>

<table>
  <tr>
    <th>id</th>
    <th>Name</th>
    <th>Username</th>
    <th>Email</th>
    <th>Bio</th>
    <th>Birthday</th>
    <th>Location</th>
  </tr>
  {{#each}}
    <tr>
      <td>{{link-to id 'users.show' this}}</td>
      <td>{{name}}</td>
      <td>{{username}}</td>
      <td>{{email}}</td>
      <td>{{bio}}</td>
      <td>{{birthday}}</td>
      <td>{{location}}</td>
    </tr>
  {{/each}}
</table>
